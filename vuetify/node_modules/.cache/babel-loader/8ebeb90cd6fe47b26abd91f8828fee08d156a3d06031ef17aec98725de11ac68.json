    <v-list-item v-for=\"[icon, text] in links\" :key=\"icon\" link>\n          <template v-slot:prepend>\n            <v-icon>{{ icon }}</v-icon>\n          </template>\n\n          <v-list-item-title>{{ text }}</v-list-item-title>\n        </v-list-item>\n      </v-list>\n      <div class=\"drawer-footer\">\n        <div class=\"drawer-footer-contents\" style=\"color:white;\">이용약관</div>\n        <div class=\"drawer-footer-contents\" style=\"color:white;\">개인정보처리방침</div>\n        <div class=\"drawer-footer-contents\" style=\"color:white;\">2023 Seeds bibim</div>\n      </div>\n\n    </v-navigation-drawer>\n\n    <v-main style=\"--v-layout-left: 350px;\"> <!-- 우측 영역 너비 비율 조정, 추후 미디어쿼리 통해 0px로 조정 필요 -->\n      <v-container class=\"py-8 px-6\" fluid>\n        <v-row>\n          <v-col v-for=\"card in cards\" :key=\"card\" cols=\"12\">\n            <v-card>\n\n              <v-list lines=\"two\">\n                <v-list-subheader>{{ card }}</v-list-subheader>\n                <template v-for=\"n in 6\" :key=\"n\">\n                  <v-list-item>\n                    <template v-slot:prepend>\n                      <v-avatar color=\"grey-darken-1\"></v-avatar>\n                    </template>\n\n                    <v-list-item-title>Message {{ n }}</v-list-item-title>\n\n                    <v-list-item-subtitle>\n                      Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nihil repellendus distinctio similique\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-divider v-if=\"n !== 6\" :key=\"`divider-${n}`\" inset></v-divider>\n                </template>\n              </v-list>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script setup>\nimport { ref } from 'vue'\n\nconst cards = ['루틴작성', '검색']\nconst links = [\n  ['mdi-inbox-arrow-down', 'HOME'],\n  ['mdi-send', '루틴 공유'],\n  ['mdi-delete', '루틴 작성'],\n  ['mdi-alert-octagon', '내 루틴 보관함'],\n  ['mdi-alert-octagon', '설정'],\n]\n\nconst drawer = ref(null)\n</script>\n\n<script>\nexport default {\n  data: () => ({\n    cards: ['루틴작성', '검색'],\n    drawer: null,\n    links: [\n      ['mdi-inbox-arrow-down', 'HOME'],\n      ['mdi-send', '루틴 공유'],\n      ['mdi-delete', '루틴 작성'],\n      ['mdi-alert-octagon', '내 루틴 보관함'],\n      ['mdi-alert-octagon', '설정'],\n    ],\n  }),\n}\n</script>"],"mappings":"AA0EA,SAASA,GAAG,QAAQ,KAAK;AAezB,MAAAC,WAAA,GAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,CAAC,MAAM,EAAE,IAAI,CAAC;IACrBC,MAAM,EAAE,IAAI;IACZC,KAAK,EAAE,CACL,CAAC,sBAAsB,EAAE,MAAM,CAAC,EAChC,CAAC,UAAU,EAAE,OAAO,CAAC,EACrB,CAAC,YAAY,EAAE,OAAO,CAAC,EACvB,CAAC,mBAAmB,EAAE,UAAU,CAAC,EACjC,CAAC,mBAAmB,EAAE,IAAI,CAAC;EAE/B,CAAC;AACH,CAAC;;;;;;;IAzBD,MAAMF,KAAK,GAAG,CAAC,MAAM,EAAE,IAAI,CAAC;IAC5B,MAAME,KAAK,GAAG,CACZ,CAAC,sBAAsB,EAAE,MAAM,CAAC,EAChC,CAAC,UAAU,EAAE,OAAO,CAAC,EACrB,CAAC,YAAY,EAAE,OAAO,CAAC,EACvB,CAAC,mBAAmB,EAAE,UAAU,CAAC,EACjC,CAAC,mBAAmB,EAAE,IAAI,CAAC,CAC5B;IAED,MAAMD,MAAM,GAAGJ,GAAG,CAAC,IAAI,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}